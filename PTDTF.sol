       // emit ShowG1("g1zb",scalar_mul(P1(),_proof.zb));
       // emit ShowG1("h1z2",scalar_mul(h1,_proof.z2));
        if(addbuff2.X!=_proof.d2.X||(addbuff2.Y!=_proof.d2.Y)) 
         {emit TrueOrFalse(false,2);return (false,2);}
        addbuff1=addition(addition(scalar_mul(scalar_mul(P1(),_proof.za), _proof.zb),scalar_mul(h1,_proof.zab)),scalar_mul(_proof.c0,x));
       // emit ShowG1("g1zazb",scalar_mul(scalar_mul(P1(),_proof.za), _proof.zb));
       // emit ShowG1("h1zab",scalar_mul(h1,_proof.zab));
       // emit ShowG1("c0x",scalar_mul(_proof.c0,x));
       // emit ShowG1("leftside",addbuff1);
    
        G1Point memory addbuff;
        addbuff=addition(scalar_mul(dataProvider[_provideIndex].bv1,mulmod(uint256(x),uint256(x),FIELD_MODULUS)),_proof.c1);
      //  emit ShowG1("cx2", scalar_mul(dataProvider[_provideIndex].bv1,mulmod(uint256(x),uint256(x),FIELD_MODULUS)));
      //  emit ShowG1("c1+cx2",addbuff);
      //  emit Showint("zazb",mulmod(_proof.za, _proof.zb, FIELD_MODULUS));
     //   emit Showint("x**2",mulmod(uint256(x),uint256(x),FIELD_MODULUS));
        if (addbuff.X!=addbuff.X||(addbuff1.Y!=addbuff.Y)) 
         {emit TrueOrFalse(false,3);return (false,3);}
        emit TrueOrFalse(true,666); 
        return (true,1);

    }
    
