        addbuff1=addition(addition(scalar_mul(_rk1,x),scalar_mul(P1(),_proof.za)),scalar_mul(h1,_proof.z1));
        if (addbuff1.X!=_proof.d1.X||(addbuff1.Y!=_proof.d1.Y)) 
            {emit TrueOrFalse(false,1);return (false,1);}
        addbuff2=addition(addition(scalar_mul(dataProvider[_provideIndex].bv2,uint256(x)),scalar_mul(P1(),_proof.zb)),scalar_mul(h1,_proof.z2));
        //emit ShowG1("addbuff2",addbuff2);
